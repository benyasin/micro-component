{
  "name": "micro-components",
  "version": "1.0.0",
  "main": "dist/runtime/rutime.js",
  "module": "dist/runtime/runtime.js",
  "types": "./types.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "types.d.ts"
  ],
  "exports": {
    "./vue/*": "./dist/components/vue/*",
    "./vue2/*": "./dist/components/vue2/*",
    "./react/*": "./dist/components/react/*",
    "./server-locales/*": "./dist/server-locales/*",
    "./server-locale-modules/*": "./dist/server-locale-modules/*",
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite -c playground/vite.config.ts",
    "dev:beta": "VITE_ENV=beta vite -c playground/vite.config.ts",
    "preview": "vite preview -c playground/vite.config.ts",
    "create-component": "tsx scripts/create-component",
    "build": "pnpm run build:runtime && ([ \"$ENABLE_SSR\" = \"true\" ] && pnpm run build:server-locales || echo \"跳过SSR locale构建\") && pnpm run build:react && pnpm run build:vue && pnpm run build:vue2 && rm -rf ./dist/server-app",
    "build:test": "([ \"$ENABLE_SSR\" = \"true\" ] && pnpm run build:server-locales || echo \"跳过SSR locale构建\") && pnpm run build:react",
    "build:runtime": "tsx --conditions=node scripts/build-runtime.ts",
    "build:server-locales": "tsx scripts/build-server-locales.ts",
    "build:vue": "tsx scripts/build-vue3.ts",
    "build:vue2": "tsx scripts/override-vue.ts --append && tsx scripts/build-vue2.ts && tsx scripts/override-vue.ts",
    "build:react": "tsx scripts/build-react.ts",
    "build:flat-locale": "tsx scripts/build-flat-locale.ts",
    "build:locale": "tsx scripts/build-locale.ts --generate",
    "fetch:locale": "tsx scripts/build-locale.ts",
    "build:online": "VITE_ENV=production pnpm run build:runtime && pnpm run jampack",
    "build:beta": "VITE_ENV=beta pnpm run build:runtime && pnpm run jampack",
    "build:test1": "VITE_ENV=test pnpm run build:runtime && pnpm run jampack ",
    "lint": "prettier --config .prettierrc.cjs --write ./src/**/*.{ts,vue,js}",
    "jampack": "jampack ./dist --exclude {server-locales,server-locale-modules}/**/**",
    "test:install": "cd test-projects/vue3 && pnpm install && cd ../react && pnpm install && cd ../vue2 && pnpm install",
    "test:dev": "concurrently \"pnpm run test:dev:vue3\" \"pnpm run test:dev:react\" \"pnpm run test:dev:vue2\"",
    "test:dev:vue3": "cd test-projects/vue3 && pnpm run dev",
    "test:dev:react": "cd test-projects/react && pnpm run dev",
    "test:dev:vue2": "cd test-projects/vue2 && pnpm run dev",
    "test:build": "cd test-projects/vue3 && pnpm run build && cd ../react && pnpm run build && cd ../vue2 && pnpm run build",
    "kill-ports": "./scripts/kill-test-ports.sh",
    "test:restart": "./scripts/restart-test-projects.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "pnpm": {
    "overrides": {}
  },
  "devDependencies": {
    "@divriots/jampack": "^0.28.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-links": "^9.1.2",
    "@storybook/vue3-vite": "^9.1.2",
    "@types/node": "^20.11.0",
    "@unocss/core": "^0.58.0",
    "@unocss/preset-attributify": "^0.58.0",
    "@unocss/preset-uno": "^0.58.0",
    "@unocss/transformer-directives": "^0.58.0",
    "@unocss/transformer-variant-group": "^0.58.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "ant-design-vue": "^4.2.3",
    "axios": "^1.6.7",
    "axios-cache-interceptor": "^1.5.3",
    "buffer": "^6.0.3",
    "clipboard": "^2.0.11",
    "concurrently": "^8.2.2",
    "dayjs": "^1.11.10",
    "events": "^3.3.0",
    "glob": "^10.3.10",
    "js-cookie": "^3.0.1",
    "less": "^4.2.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "postcss-prefix-selector": "^1.16.0",
    "postcss-rtlcss": "^5.1.0",
    "qrcode": "^1.5.3",
    "query-string": "^8.1.0",
    "queue": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.70.0",
    "shelljs": "^0.8.5",
    "storybook": "^9.1.2",
    "swiper": "^11.0.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.0",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-vue2": "^2.0.3",
    "vite-plugin-windicss": "^1.8.10",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.15",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.2.5",
    "vue-template-compiler": "^2.7.14",
    "vue3-text-clamp": "^0.1.1"
  },
  "publishConfig": {
    "registry": "https://security-jfrog.bitget.tools:8506/artifactory/api/npm/npmjs-registry-local-repos/"
  },
  "engines": {
    "npm": ">=8.0.0",
    "pnpm": ">=7.12.2",
    "node": ">=16.16.0"
  },
  "packageManager": "pnpm@10.13.1"
}